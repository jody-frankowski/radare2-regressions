#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME='ELF: simple.elf - open'
FILE=../../binaries/elf/simple.elf
ARGS=''
CMDS="q!"
EXPECT=''
run_test

NAME='ELF: simple.elf - code'
FILE=../../binaries/elf/simple.elf
ARGS=''
CMDS="pid 4 @ entry0"
EXPECT='0x08000060        b990000008  mov ecx, 0x8000090
0x08000065        ba0d000000  mov edx, 0xd
0x0800006a        bb01000000  mov ebx, 0x1
0x0800006f        b804000000  mov eax, 0x4
'
run_test
