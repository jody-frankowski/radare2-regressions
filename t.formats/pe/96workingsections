#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME='PE: corkami 96workingsections.exe - open'
FILE=../../bins/pe/96workingsections.exe
ARGS=''
CMDS="q!"
EXPECT=''
run_test

BROKEN=1
IGNORE_ERR=
NAME='PE: corkami 96workingsections.exe - entrypoint'
