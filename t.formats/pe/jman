#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME='MZ: jman.exe - open'
FILE=../../binaries/pe/jman.exe
ARGS=''
CMDS="q!"
EXPECT=''

run_test

NAME='MZ: jman.exe - entrypoint'
BROKEN=1
FILE=../../binaries/pe/jman.exe
ARGS=''
CMDS="pi 1"
EXPECT='mov ax, 0x2df3
'
run_test
