#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME='PE: corkami CoST.exe - open'
FILE=../../binaries/pe/CoST.exe
ARGS=''
CMDS="q!"
IGNORE_ERR=1
EXPECT=''
run_test

NAME='PE: corkami CoST.exe - entrypoint'
FILE=../../binaries/pe/CoST.exe
ARGS='-n'
IGNORE_ERR=1
CMDS="!rabin2 -eq ${FILE}"
EXPECT='0x00000000
'
run_test

NAME='PE: corkami CoST.exe - sections list'
FILE=../../binaries/pe/CoST.exe
ARGS='-n'
IGNORE_ERR=1
CMDS="!rabin2 -S ${FILE}"
EXPECT='[Sections]

0 sections
'
run_test
